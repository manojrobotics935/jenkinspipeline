pipeline {
	agent any;
	stages {
	  stage ('BUILD') {
           steps {
		echo "This is my first build in pipeline"
		echo "THIS IS SAMPLE"
              }	
           }
	  stage ('DEPLOY') {
           steps {
		echo "This is my first DEPLOY in pipeline"
		echo "THIS IS SAMPLE IN DEPLOY"
		sh '''
			sleep 5
			exit 0
		'''	
              }	
         }
	  stage ('TEST') {
           steps {
		echo "This is my first TEST in pipeline"
		echo "THIS IS SAMPLE IN TEST"
              }	
           }
	}
}
